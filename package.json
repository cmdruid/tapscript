{
  "name": "@cmdcode/tapscript",
  "version": "1.2.1",
  "description": "A basic library for working with Tapscript, signatures and Bitcoin transactions.",
  "type": "module",
  "main": "./dist/main.js",
  "unpkg": "./dist/bundle.min.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/module.js",
    "require": "./dist/main.js"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf dist/* coverage .nyc_output",
    "start": "node --trace-deprecation --no-warnings --loader ts-node/esm",
    "scratch": "yarn start test/scratch.ts",
    "lint": "eslint . --ext .ts",
    "fix": "eslint --fix . --ext .ts",
    "tape": "yarn start test/tape.ts",
    "test": "nyc --reporter=lcov --require ts-node/register yarn tape | tap-spec; nyc report ---reporter=text",
    "types": "tsc",
    "release": "yarn test && yarn clean && yarn types && yarn lint && yarn build"
  },
  "keywords": [
    "bitcoin",
    "transaction",
    "psbt",
    "serialize",
    "encode",
    "decode",
    "validate"
  ],
  "author": "Christopher Scott",
  "license": "CC-BY-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/tapscript.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "devDependencies": {
    "@cmdcode/keylink": "^1.4.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.14.0",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nyc": "^15.1.0",
    "rollup": "^3.2.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "zod": "^3.20.6"
  },
  "dependencies": {
    "@cmdcode/buff-utils": "^1.6.6",
    "@cmdcode/crypto-utils": "^1.6.4"
  },
  "browser": {
    "crypto": true
  }
}
