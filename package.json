{
  "name": "@scrow/tapscript",
  "version": "2.0.3",
  "description": "A development build of tapscript. Built for escrow.",
  "type": "module",
  "main": "./dist/main.cjs",
  "unpkg": "./dist/browser.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/module.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/main.cjs"
      }
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf dist/* coverage .nyc_output",
    "scratch": "tsx test/scratch.ts",
    "lint": "eslint . --ext .ts",
    "test": "nyc --reporter=lcov tsx test/tape.ts | tap-spec; nyc report ---reporter=text",
    "types": "tsc",
    "release": "yarn test && yarn clean && yarn types && yarn lint && yarn build"
  },
  "keywords": [
    "bitcoin",
    "transaction",
    "psbt",
    "serialize",
    "encode",
    "decode",
    "validate"
  ],
  "author": "Christopher Scott",
  "license": "CC-BY-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/tapscript.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "devDependencies": {
    "@cmdcode/buff": "^2.0.2",
    "@cmdcode/crypto-tools": "^2.4.9",
    "@noble/curves": "^0.9.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.14.0",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nyc": "^15.1.0",
    "rollup": "^3.2.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "zod": "^3.20.6"
  }
}
