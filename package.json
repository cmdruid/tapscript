{
  "name": "@cmdcode/tapscript",
  "version": "1.4.6",
  "description": "A basic library for working with Tapscript, signatures and Bitcoin transactions.",
  "type": "module",
  "main": "./dist/main.cjs",
  "unpkg": "./dist/bundle.min.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "lint": "eslint",
    "load": "tsx --tsconfig ./test/tsconfig.json",
    "release": "yarn lint && yarn test && yarn build",
    "scratch": "yarn load test/scratch.ts",
    "test": "yarn tsc && yarn load test/tape.ts | tap-spec"
  },
  "keywords": [
    "bitcoin",
    "transaction",
    "psbt",
    "serialize",
    "encode",
    "decode",
    "validate"
  ],
  "author": "Christopher Scott",
  "license": "CC-BY-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/tapscript.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "devDependencies": {
    "@cmdcode/buff": "^2.2.5",
    "@cmdcode/core-cmd": "^1.6.3",
    "@cmdcode/crypto-tools": "^2.7.7",
    "@noble/curves": "^1.6.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^22.4.1",
    "@types/tape": "^5.6.4",
    "rollup": "^4.21.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.7.5",
    "tslib": "^2.7.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  }
}
